<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AFRICANSHIPPINGAPP.NoActionBar"
        tools:targetApi="31" >
        <activity
            android:name=".ui.ViewGoods.StoreGoodsDetails"
            android:exported="false" />
        <activity
            android:name=".ui.ViewGoods.ViewStoreGoods"
            android:exported="false" />
        <activity
            android:name=".ui.EnterGoods.StoreGoodsInsertion"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.AFRICANSHIPPINGAPP.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.EnterGoods.InsertionActivity"
            android:exported="true" />
        <activity
            android:name=".ui.ViewGoods.ViewTruckGoods"
            android:exported="true" />
        <activity
            android:name=".ui.ViewGoods.TruckGoodsDetailsActivity"
            android:exported="true" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>